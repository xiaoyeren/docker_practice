{
  "title": "Docker -- 从入门到实践",
  "author": "yeasy",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "GitHub": "https://github.com/yeasy/docker_practice"
    }
  },
  "plugins": [
    "-livereload",
    "image-captions",
    "github-buttons",
    "page-treeview"
  ],
  "pluginsConfig": {
    "image-captions": {
      "attributes": {
        "width": "600"
      },
